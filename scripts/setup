#!/usr/bin/env bash
# http://redsymbol.net/articles/unofficial-bash-strict-mode/
set -euo pipefail
IFS=$'\n\t'

scripts/bootstrap


gdrive --service-account $GDRIVE_ACCOUNT_CREDENTIALS_JSON download --force '1W8z5a34gEs3kGyxfqIQ5_VBdP2F8iWYD'
