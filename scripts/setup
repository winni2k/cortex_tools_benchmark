pushd down
gdrive --service-account $GDRIVE_ACCOUNT_CREDENTIALS_JSON download --force $FIXTURE_PACKAGE_GDRIVE_ID
popd
tar -xf down/cortex-tools-fixtures.tar.bz2

scripts/build_fixtures
